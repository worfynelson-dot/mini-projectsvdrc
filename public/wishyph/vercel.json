{
  "rewrites": [
    {
      "source": "/wishyph",
      "destination": "/wishyph/index.html"
    },
    {
      "source": "/wishyph/",
      "destination": "/wishyph/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "PUT,POST,DELETE,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "*" }
      ]
    }
  ]
}
